{
  "name": "sufod",
  "version": "1.0.0",
  "scripts": {
    "lint": "npm run lint:check",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "echo 'TODO: Add a test runner and then update this command'",
    "build": "npm run clean && tsc",
    "serve": "node dist/server.js",
    "start": "npm run build && npm run copy-files && npm run serve",
    "dev": "nodemon src/server.ts",
    "copy-files": "copyfiles -u 1 src/public/* dist/",
    "clean": "rimraf dist/"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "copyfiles": "^2.4.1",
    "eslint": "^7.19.0",
    "nodemon": "^2.0.16",
    "rimraf": "^3.0.2"
  },
  "engines": {
    "node": ">=14"
  },
  "displayName": "Sufod",
  "description": "A Dofus version of Sutom",
  "author": "savari-o",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/oliviersvrn/sufod"
  },
  "bugs": "https://github.com/oliviersvrn/sufod/issues",
  "keywords": [
    "wordle",
    "sutom",
    "dofus"
  ],
  "dependencies": {
    "express": "^4.18.1",
    "ts-node": "^10.7.0",
    "tslint": "^6.1.3",
    "typescript": "^4.6.4"
  }
}
